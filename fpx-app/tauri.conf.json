{
  "build": {
    "beforeBuildCommand": "pnpm build:fpx-studio",
    "beforeDevCommand": "pnpm dev:api & pnpm dev:frontend",
    "devPath": "http://localhost:5173",
    "distDir": "../desktop/dist"
  },
  "package": {
    "productName": "fpx-tauri",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "open": true
      },
      "path": {
        "all": true
      },
      "shell": {
        "all": true
      },
      "http": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.fiberplane.fpx",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "focus": false,
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "fpx",
        "width": 800
      }
    ]
  }
}
